  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-GRP0KxsrdQThJOW11A1B6A" name="governo,_1JY30nlKEe-qo_w2p3QS1w" guid="-GRP0KxsrdQThJOW11A1B6A">
